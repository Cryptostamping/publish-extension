{
  "short_name": "Cryptostamping",
  "name": "Cryptostamping",
  "description": "Let your NFTs add value to the internet.",
  "icons": {
    "16": "logo16.png",
    "32": "logo32.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "permissions": ["activeTab", "tabs", "storage"],
  "optional_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["chrome/content.js"],
      "all_frames": false,
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "chrome/background.js"
  }
}
