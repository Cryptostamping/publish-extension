{
  "short_name": "Cryptostamping",
  "name": "Cryptostamping",
  "description": "Let your NFTs add value to the internet.",
  "icons": {
    "16": "logo16.png",
    "32": "logo32.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "version": "1.0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "<all_urls>"],
      "js": ["chrome/content.js"],
      "all_frames": false,
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Connect to Metamask"
  },
  "background": {
    "scripts": ["chrome/background.js"],
    "persistent": false
  },
  "permissions": ["<all_urls>", "activeTab", "tabs", "storage"],
  "web_accessible_resources":[
    "/static/css/*", "/static/js/*","/icons/*", "/logo_favicon.svg"
  ]
}